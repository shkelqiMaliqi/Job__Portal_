ord} onChange={handleChange} />\r\n        </Form.Group>\r\n\r\n        <Button variant='primary' onClick={handleUpdate}>Update</Button>\r\n      </Form>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default UpdateUser;\r\n","C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\Jobs_Repository\\Jobs_Listings.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\User_Updates\\Users.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\BusinessDetails\\BusinessUsers.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\Courses\\Courses_List.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\Admin_Bashboard\\Staff\\Staff.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\Jobs_Repository\\JobCategoriesCity.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\Jobs_Repository\\JobCategories.js",[],"C:\\Users\\Lenovo\\OneDrive\\Desktop\\Job__Portal_\\UI\\jobportal\\src\\Jobs_Repository\\JobCategoriesSchedule.js",[],{"ruleId":"162","replacedBy":"163"},{"ruleId":"164","replacedBy":"165"},{"ruleId":"166","severity":1,"message":"167","line":58,"column":52,"nodeType":"168","messageId":"169","endLine":58,"endColumn":72},{"ruleId":"166","severity":1,"message":"170","line":67,"column":49,"nodeType":"168","messageId":"169","endLine":67,"endColumn":66},{"ruleId":"166","severity":1,"message":"171","line":70,"column":47,"nodeType":"168","messageId":"169","endLine":70,"endColumn":63},{"ruleId":"172","severity":1,"message":"173","line":12,"column":6,"nodeType":"174","endLine":12,"endColumn":8,"suggestions":"175"},{"ruleId":"172","severity":1,"message":"176","line":10,"column":6,"nodeType":"174","endLine":10,"endColumn":8,"suggestions":"177"},{"ruleId":"178","severity":1,"message":"179","line":74,"column":11,"nodeType":"180","messageId":"181","endLine":74,"endColumn":30},{"ruleId":"178","severity":1,"message":"18